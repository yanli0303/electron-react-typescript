{
  "compilerOptions": {
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "checkJs": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "../renderer/public",
    "skipLibCheck": true,
    "strict": true,
    "target": "es5",
    "typeRoots": ["./node_modules/@types/"]
  },
  "include": ["src"]
}
